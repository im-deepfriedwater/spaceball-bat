[application]

config/name="Baseball Bat"
run/main_scene="res://scenes/MainBaseballGame.tscn"
config/icon="res://icon.png"

[aseprite]

import/import_plugin/enable_automatic_importer=true

[autoload]

Discord="*res://addons/discord_game_sdk/discord.gd"
DiscordRichPresenceSingleton="*res://scripts/singletons/DiscordRichPresenceSingleton.gd"
SceneSwitcherSingleton="*res://scripts/singletons/SceneSwitcherSingleton.gd"
BaseballEventsSingleton="*res://scripts/singletons/event_singletons/BaseballEventsSingleton.gd"
Globals="*res://scripts/singletons/globals.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="viewport"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg", "res://addons/discord_game_sdk/plugin.cfg" )

